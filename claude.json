{
  "editorMode": "vim",
  "verbose": false,
  "autoCompactEnabled": true,
  "todoFeatureEnabled": true,
  "defaultOutputStyle": "bro",
  "mcpServers": {
    "MCP_DOCKER": {
      "command": "docker",
      "type": "stdio",
      "args": [
        "mcp",
        "gateway",
        "run"
      ]
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--init",
        "--security-opt",
        "no-new-privileges",
        "--cpus",
        "1",
        "--memory",
        "2Gb",
        "--pull",
        "never",
        "techgopal/ultrafast-mcp-sequential-thinking:latest",
        "sequential-thinking-server",
        "--transport",
        "stdio",
        "--port",
        "50101",
        "--max-thoughts",
        "200",
        "--max-branches",
        "50"
      ]
    },
    "archon": {
      "type": "http",
      "url": "http://localhost:8051/mcp"
    },
     "serena": {
       "type": "stdio",
       "command": "uvx",
       "args": [
         "--from",
         "git+https://github.com/oraios/serena",
         "serena-mcp-server",
         "--context",
         "ide-assistant",
         "--mode",
         "interactive",
         "editing",
         "--enable-web-dashboard",
         "false",
         "--enable-gui-log-window",
         "false"
       ]
     }
  }
}
