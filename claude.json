{
  "editorMode": "vim",
  "verbose": false,
  "autoCompactEnabled": true,
  "todoFeatureEnabled": true,
  "mcpServers": {
    "MCP_DOCKER":{
      "command":"docker",
      "type": "stdio",
      "args":[
        "mcp",
        "gateway",
        "run"
      ]
    },
    "semgrep": {
      "type": "http",
      "url": "https://mcp.semgrep.ai/mcp"
    },
    "cogitare": {
      "command": "docker",
      "type": "stdio",
      "args": [
        "run",
        "--rm",
        "-i",
        "--init",
        "--security-opt",
        "no-new-privileges",
        "--cpus",
        "1",
        "--memory",
        "2Gb",
        "--pull",
        "always",
        "ghcr.io/anntnzrb/cogitare:latest"
      ]
    }
  }
}
