{
  "editorMode": "vim",
  "verbose": false,
  "autoCompactEnabled": true,
  "todoFeatureEnabled": true,
  "defaultOutputStyle": "bro",
  "mcpServers": {
    "MCP_DOCKER": {
      "command": "docker",
      "type": "stdio",
      "args": [
        "mcp",
        "gateway",
        "run"
      ]
    },
    "cogitare": {
      "command": "docker",
      "type": "stdio",
      "args": [
        "run",
        "--rm",
        "-i",
        "--init",
        "--security-opt",
        "no-new-privileges",
        "--cpus",
        "1",
        "--memory",
        "2Gb",
        "--pull",
        "always",
        "ghcr.io/anntnzrb/cogitare:latest"
      ]
    }
  }
}
